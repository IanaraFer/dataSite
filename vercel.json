{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/",
      "dest": "website/index.html"
    },
    {
      "src": "/((?!api).+)",
      "dest": "website/$1"
    }
  ]
}
